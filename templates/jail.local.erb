[DEFAULT]

<% if @ignoreip -%> 
ignoreip = <%= @ignoreip.join(" ") %>
<% end -%>

<% if @bantime -%> 
bantime = <%= @bantime %>
<% end -%>

<% if @findtime  -%> 
findtime  = <%= @findtime %>
<% end -%>

<% if @maxretry -%> 
maxretry = <%= @maxretry %>
<% end -%>

<% if @backend -%> 
backend = <%= @backend %>
<% end -%>

<% if @usedns -%> 
usedns = <%= @usedns %>
<% end -%>

<% if @destemail -%> 
destemail = <%= @destemail %>
<% end -%>

<% if @email_sender -%> 
sender = <%= @email_sender %>
<% end -%>

<% if @mta -%> 
mta = <%= @mta %>
<% end -%>

<% if @protocol -%> 
protocol = <%= @protocol %>
<% end -%>

<% if @chain -%> 
chain = <%= @chain %>
<% end -%>

<% if @banaction -%> 
banaction = <%= @banaction %>
<% end -%>

<% if @action -%> 
action = <%= @action %>
<% end -%>

